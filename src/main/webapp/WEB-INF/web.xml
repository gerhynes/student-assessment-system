<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>RegisterUser</servlet-name>
    <display-name>RegisterUser</display-name>
    <description></description>
    <servlet-class>com.example.web.RegisterUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginUser</servlet-name>
    <display-name>LoginUser</display-name>
    <description></description>
    <servlet-class>com.example.web.LoginUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PopulateAdminDashboard</servlet-name>
    <display-name>PopulateAdminDashboard</display-name>
    <description></description>
    <servlet-class>com.example.web.PopulateAdminDashboard</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PopulateInstructorDashboard</servlet-name>
    <display-name>PopulateInstructorDashboard</display-name>
    <description></description>
    <servlet-class>com.example.web.PopulateInstructorDashboard</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PopulateStudentDashboard</servlet-name>
    <display-name>PopulateStudentDashboard</display-name>
    <description></description>
    <servlet-class>com.example.web.PopulateStudentDashboard</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterUser</servlet-name>
    <url-pattern>/RegisterUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginUser</servlet-name>
    <url-pattern>/LoginUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PopulateAdminDashboard</servlet-name>
    <url-pattern>/PopulateAdminDashboard</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PopulateInstructorDashboard</servlet-name>
    <url-pattern>/PopulateInstructorDashboard</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PopulateStudentDashboard</servlet-name>
    <url-pattern>/PopulateStudentDashboard</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <error-page>
    <exception-type>java.lang.Exception</exception-type>
    <location>/error.jsp</location>
  </error-page>
</web-app>
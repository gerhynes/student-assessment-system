<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>CourseServlet</servlet-name>
    <display-name>CourseServlet</display-name>
    <description></description>
    <servlet-class>com.example.web.CourseServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetCourseController</servlet-name>
    <display-name>GetCourseController</display-name>
    <description></description>
    <servlet-class>com.example.web.GetCourseController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetCoursesController</servlet-name>
    <display-name>GetCoursesController</display-name>
    <description></description>
    <servlet-class>com.example.web.GetCoursesController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CourseServlet</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetCourseController</servlet-name>
    <url-pattern>/getCourse</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetCoursesController</servlet-name>
    <url-pattern>/getCourses</url-pattern>
  </servlet-mapping>
</web-app>


